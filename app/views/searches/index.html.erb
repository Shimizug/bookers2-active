<table class="table table-hover table-inverse">
  <% if @range == "User" %>
    <tbody>
      <tr><th>Name</th></tr>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <tr>
          <th>Title</th>
          <th>Option</th>
        </tr>
      <% @books.each do |book| %>
        
        <tr>
          <td><%= book.title %></td>
          <td><%= book.body %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>